{
  "name": "express-htaccess-middleware",
  "version": "0.1.0",
  "description": "An express middleware that interprets .htaccess rewrite rules.",
  "main": "lib/middleware.js",
  "scripts": {
    "test": "./node_modules/.bin/_mocha",
    "lint": "./node_modules/.bin/eslint lib/*.js",
    "cover": "./node_modules/.bin/istanbul cover --report html node_modules/.bin/_mocha --check-leaks",
    "coveralls": "./node_modules/.bin/istanbul cover --report lcov node_modules/.bin/_mocha && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "keywords": [
    "htaccess",
    "rewrite",
    "mod_rewrite",
    "redirect",
    "redirection",
    "express"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/rundef/node-express-htaccess-middleware.git"
  },
  "author": "Mickael Burguet <www.rundef.com>",
  "license": "MIT",
  "engines": {
    "node": ">= 0.12.0"
  },
  "dependencies": {
    "app-root-path": "^1.2.1",
    "chalk": "^1.1.3",
    "htaccess-parser": "0.1.0",
    "lodash": "^4.13.1"
  },
  "devDependencies": {
    "async": "^2.0.0-rc.6",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.13.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.9.2",
    "express": "^4.14.0",
    "istanbul": "^0.4.3",
    "mocha": "^2.5.3",
    "supertest": "^1.2.0"
  }
